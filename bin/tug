#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'tug==0.2.0','console_scripts','tug'
__requires__ = 'tug==0.2.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('tug==0.2.0', 'console_scripts', 'tug')()
    )
